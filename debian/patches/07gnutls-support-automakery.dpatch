+	if [ ! -d .libs ]; then mkdir .libs; fi
+	rm -f .libs/libperl_orig.a
+	if [ x$(LIBPERL_A) = x ]; then touch .libs/libperl_orig.a; else $(LN_S) $(LIBPERL_A) .libs/libperl_orig.a; fi
